{
  "name": "fruit-ninja",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/henryouly/fruit-ninja",
  "author": "Henry Ou <henryouly@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "parcel build index.js",
    "lint": "eslint .",
    "link-local": "yalc link"
  },
  "alias": {
    "scripts": "./source/scripts",
    "images": "./source/images",
    "sound": "./source/sound"
  },
  "devDependencies": {
    "eslint": "^7.17.0",
    "eslint-config-google": "^0.9.1",
    "parcel-bundler": "^1.12.4",
    "yalc": "^1.0.0-pre.49"
  },
  "eslintConfig": {
    "extends": "google",
    "rules": {
      "require-jsdoc": 0,
      "valid-jsdoc": 0,
      "max-len": "off",
      "no-unused-vars": "off",
      "no-tabs": "off",
      "indent": [2, "tab"]
    },
    "env": {
      "es6": true,
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    }
  },
  "eslintIgnore": [
    "dist/"
  ]
}
